{
  "expo": {
    "name": "Fitaraise",
  
    "displayName": "Fitaraise",
    "version": "1.0.0",
    "sdkVersion": "48.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "extra": {
      "eas": {
        "projectId": "d722df05-12ff-4f32-bea1-c9c9d5d7976c"
      }
    },
    
    "splash": {
      "image": "./assets/fitaraise.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/d722df05-12ff-4f32-bea1-c9c9d5d7976c"
    },
    "android": {
      "runtimeVersion": "1.0.0",
      "package": "com.sandeepsanand.fitaraise_windowsupdatemain",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "sandeepsanand"
        }
      ],
      "expo-localization",
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app"
    ],
    "ios": {
      "runtimeVersion": {
        "policy": "sdkVersion"
      },
 
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
     }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-1007295888890571~9597988497"
  }
}
