{
  "expo": {
    "name": "Fitaraise",
    "slug": "fitaraise_windows-update-main",
    "version": "1.0.0",
    "sdkVersion": "48.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/fitaraise.png",
      "resizeMode": "cover",
      "backgroundColor": "#fffff"
    },
    "android": {
      "package": "com.sandeepsanand.fitaraise_windowsupdatemain",
      
      "runtimeVersion": "1.0.0",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fffff"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/624ffd43-5a6e-4881-81c1-e1a7a72a9b9c"
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "sandeepsanand"
        }
      ],
      "expo-localization",
      "@react-native-google-signin/google-signin",
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "624ffd43-5a6e-4881-81c1-e1a7a72a9b9c"
      }
    },
    "ios": {
      "runtimeVersion": {
        "policy": "sdkVersion"
      },
      "bundleIdentifier": "com.sandeepsanand.fitaraise_windowsupdatemain",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
     }
    }
  }
}
